<li id="<%= feed_item.id %>">
  
  <span class="timestamp">
    Post√© le <%= feed_item.created_at.strftime("%d, %b, %Y") %>.
  </span></br>
  <div class= "tings">
  <span class="pull_up">Pull up:   <%= feed_item.pull_up %>kgs</span></br>
  <span class="deadlift">Deadlift:   <%= feed_item.deadlift %>kgs</span></br>
  <span class="rows">Row:   <%= feed_item.row %>kgs</span></br>
  <span class="bicep_curl">Bicep curl:   <%= feed_item.bicep_curl %>kgs</span></br>
  <span class="hammer">Hammer:   <%= feed_item.hammer %>kgs</span>
 </div>
 <% if current_user?(feed_item.user) %>
    <%= link_to "delete", feed_item, method: :delete,
                                     data: { confirm: "vous etes sure?" },
                                     title: feed_item %>
  <% end %>
</li>